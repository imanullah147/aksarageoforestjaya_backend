{
  "name": "aksarageoforestjaya",
  "version": "1.0.0",
  "description": "Backend service for Aksara Geoforest Jaya built with Express, Sequelize, and PostgreSQL",
  "main": "app.js",
  "type": "commonjs",
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "dev": "nodemon app.js",
    "db:migrate": "npx sequelize-cli db:migrate --config src/config/config.js --env development",
    "db:seed": "npx sequelize-cli db:seed:all --config src/config/config.js",
    "db:rollback": "npx sequelize-cli db:migrate:undo --config src/config/config.js"
  },
  "keywords": [
    "express",
    "sequelize",
    "postgresql",
    "jwt",
    "api",
    "backend"
  ],
  "author": "Aksara Geoforest Jaya Dev Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-safe": "^9.1.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "streamifier": "^0.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.2"
  }
}
